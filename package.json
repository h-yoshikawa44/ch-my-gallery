{
  "name": "ch-my-gallery",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint-check": "yarn lint:stylelint && yarn check:prettier",
    "lint:stylelint": "yarn run -s stylelint './css/**/*.css'",
    "check:prettier": "yarn run -s prettier --check './**/*.{html,js,ts,json}'",
    "fix": "yarn fix:stylelint && yarn fix:prettier",
    "fix:stylelint": "yarn lint:stylelint --fix",
    "fix:prettier": "yarn check:prettier --write"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "vite": "^2.6.4"
  },
  "volta": {
    "node": "16.13.0",
    "yarn": "1.22.17"
  }
}
